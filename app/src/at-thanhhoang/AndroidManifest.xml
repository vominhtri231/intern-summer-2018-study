<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asiantech.internship.summer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewActivity"/>
        <activity android:name=".exercise_fragment_activity.FragmentActivity" />
        <activity android:name=".exercise_fragment_activity.ResultActivity" />
        <activity android:name=".timeline.TimelineActivity" />
        <activity android:name=".viewpager.ViewPagerActivity"/>
        <activity android:name=".asynctask_thread_handler.AsyncTaskThreadHandlerActivity" />
    </application>
</manifest>
