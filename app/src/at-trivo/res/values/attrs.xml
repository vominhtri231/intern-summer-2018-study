<resources>
    <declare-styleable name="EventView">
        <attr name="message" format="string" />
        <attr name="image_src" format="string" />
        <attr name="time" format="string" />
        <attr name="location" format="string" />

    </declare-styleable>
    <declare-styleable name="DayView">
        <attr name="dayNumber" format="string" />
        <attr name="day" format="string" />
        <attr name="isToday" format="boolean" />
        <attr name="isHasEvent" format="boolean" />
    </declare-styleable>
</resources>
